#pragma rtGlobals=1		// Use modern global access method.macro dofit(wvpc)	String wvpc	Prompt wvpc,"choose PC wave to fit",popup,WaveList("*",";","")	";_none_"	|the names of the waves to store the data in	String wvpa wvpcSE wvpcfit wvpccoeffs wvpccoeffSE	if  (cmpstr(wvpc[0,3],"ideal") == 0)		wvpa = wvpc[0,strlen(wvpc)-2]+"pa";	else		wvpa = wvpc[0,strlen(wvpc)-3]+"pa";	endif		print wvpa	wvpcSE = wvpc+"SE";	wvpcfit = wvpc+"fit";	wvpccoeffs = wvpc+"coeffs";	wvpccoeffSE = wvpc+"coeffSE";		make /o/n=1 $wvpccoeffs $wvpccoeffSE	duplicate /o $wvpa $wvpcfit	duplicate /o $wvpcSE wt	| set initial values	$wvpccoeffs[0] = .5;	$wvpccoeffs[1] = 1;	$wvpccoeffs[2] = .5;	|$wvpccoeffs[3] = .5;			| initialize	variable V_FitTol; 	variable V_FitMaxIters; | this forces igor to make many iterations	V_FitTol=0.00001 		V_FitMaxIters = 500;		| with weights	variable index=0	do		if (wt[index] == 0)			wt[index] = mean(wt,-inf,inf);		endif			index += 1;		while (index < (numpnts(wt)));	wt = 1/wt;		print wvpc	Funcfit linear $wvpccoeffs $wvpc /X=$wvpa /W=wt /D=$wvpcfit		$wvpccoeffSE = W_sigma;	edit $wvpccoeffs $wvpccoeffSE $wvpc $wvpcfit $wvpa	 killwaves W_sigma wt;	 end;macro namewaves()	| load data file	loadwave /T/D/O		| get the file name, etc.	string initials session imagetype;	if (cmpstr(lowerstr(S_filename[0,4]),"ideal") == 0) 			initials = S_filename[0,4];		session = S_filename[5];	else		initials = S_filename[0,2];	       session = S_filename[3];	endif	imagetype = S_filename[strlen(S_filename)-8,strlen(S_filename)-6];		  end;function/D parabola(w,x)wave/D w;variable/D x;	return (w[0]*((x-w[1])^w[2]));end;function/D quick(w,x)wave/D w;variable/D x;	return (100-w[0]*exp(-1*((x-w[1])^w[2])));end;function/D logarithm(w,x)wave/D w;variable/D x;	return (w[0]*ln(x/w[1]));end;function/D hyptangent(w,x)wave/D w;variable/D x;	return (w[0] + w[1]*tanh(x));end;function/D linear(w,x)wave/D w;variable/D x;	return (w[0]*log(x/100) + 100);end;Proc PCvsPAStyle() : GraphStyle	PauseUpdate; Silent 1		| modifying window...	ModifyGraph/Z mode=3	ModifyGraph/Z marker[0]=8,marker[1]=19,marker[2]=16,marker[3]=17,marker[4]=18,marker[5]=15	ModifyGraph/Z rgb=(0,0,0)	ModifyGraph/Z msize=4	ModifyGraph/Z opaque=1	ModifyGraph/Z tick=2	ModifyGraph/Z mirror=2	ModifyGraph/Z lblMargin(left)=14,lblMargin(bottom)=13	ModifyGraph/Z axOffset(left)=1.5,axOffset(bottom)=1.4	ModifyGraph/Z lblLatPos(left)=12,lblLatPos(bottom)=1	Label/Z left "\\F'Times'\\Z16\\f01Percent Correct "	Label/Z bottom "\\F'Times'\\Z16\\f01Percent Agreement"	SetAxis/Z left 0,100	SetAxis/Z bottom -1,100EndMacroWindow IdealScaledContrastPCvsPA() : Graph	PauseUpdate; Silent 1		| building window...	Display /W=(228,63,686,459) idealL10SCcond1pcfit vs idealL10SCcond1pa as "IdealScaledContrastPCvsPA"	Append idealL10SCcond2pcfit vs idealL10SCcond2pa	Append idealL10SCcond3pcfit vs idealL10SCcond3pa	Append idealL10SCcond4pcfit vs idealL10SCcond4pa	Append idealL10SCcond5pcfit vs idealL10SCcond5pa	Append idealL10SCcond6pcfit vs idealL10SCcond6pa	Append idealL10SCcond7pcfit vs idealL10SCcond7pa	Append idealL10SCcond8pcfit vs idealL10SCcond8pa	Append idealL10SCcond9pcfit vs idealL10SCcond9pa	Append idealL10SCcond10pcfit vs idealL10SCcond10pa	Append idealL10SCcond11pcfit vs idealL10SCcond11pa	Append jmgL10H1pc vs jmgL10H1pa	Append jmgL10M1pc vs jmgL10M1pa	Append jmgL10L1pc vs jmgL10L1pa	Append pjbL10H1pc vs pjbL10H1pa	Append pjbL10L1cond1pc vs pjbL10L1cond1pa	Append pjbL10M1cond1pc vs pjbL10M1cond1pa	ModifyGraph mode(jmgL10H1pc)=3,mode(jmgL10M1pc)=3,mode(jmgL10L1pc)=3,mode(pjbL10H1pc)=3	ModifyGraph mode(pjbL10L1cond1pc)=3,mode(pjbL10M1cond1pc)=3	ModifyGraph marker(idealL10SCcond1pcfit)=8,marker(idealL10SCcond2pcfit)=8,marker(idealL10SCcond3pcfit)=16	ModifyGraph marker(idealL10SCcond4pcfit)=17,marker(idealL10SCcond5pcfit)=18,marker(idealL10SCcond6pcfit)=15	ModifyGraph marker(idealL10SCcond9pcfit